Cell[BoxData[{
 RowBox[{
  RowBox[{"ld", "=", 
   RowBox[{"Simplify", "[", 
    FractionBox[
     RowBox[{"2", "R", "*", 
      RowBox[{"Sin", "[", 
       FractionBox["\[Pi]", "n"], "]"}]}], 
     RowBox[{"Cos", "[", 
      RowBox[{
       FractionBox["\[Pi]", "2"], "-", "\[Theta]"}], "]"}]], "]"}]}], 
  RowBox[{"(*", "diagonal", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"ll", "=", 
