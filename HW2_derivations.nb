Cell[CellGroupData[{

Cell["Weight of the Truss", "Subsubsection",
 CellChangeTimes->{{3.7907776488869333`*^9, 
  3.7907776752995186`*^9}},ExpressionUUID->"16499767-9f8d-4447-8145-\
3b27f79a909f"],

Cell[TextData[{
 "The weight of the truss is the per unit truss cell which is length ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["l", "l"], TraditionalForm]],ExpressionUUID->
  "253b1e68-d8e1-4b8a-b29e-1ed9f27ac452"]
}], "Text",
 CellChangeTimes->{{3.7907776969352407`*^9, 
  3.7907777321047225`*^9}},ExpressionUUID->"337d2e58-b2b2-49f0-aad2-\
7056748c7014"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"w", "=", 
  FractionBox["M", "ll"]}]], "Input",
 CellChangeTimes->{{3.7907776776452446`*^9, 3.7907776931329947`*^9}, {
  3.790777781082031*^9, 3.7907777874542665`*^9}, {3.790778116105584*^9, 
  3.790778119702679*^9}},
 CellLabel->
  "In[224]:=",ExpressionUUID->"2b77e88f-70e4-42d5-a50d-9f4079c2b70c"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"Csc", "[", 
    FractionBox["\[Pi]", "n"], "]"}], " ", 
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"8", " ", "R", " ", "\[Rho]", " ", 
       RowBox[{"Cot", "[", "\[Theta]", "]"}], " ", 
       RowBox[{"Sin", "[", 
        FractionBox["\[Pi]", "n"], "]"}], " ", 
       SqrtBox[
        FractionBox[
         RowBox[{"P", " ", 
          SuperscriptBox["R", "2"], " ", 
          SuperscriptBox[
           RowBox[{"Cot", "[", "\[Theta]", "]"}], "2"], " ", 
          SuperscriptBox[
           RowBox[{"Sin", "[", 
            FractionBox["\[Pi]", "n"], "]"}], "2"]}], 
         RowBox[{"EE", " ", "n"}]]]}], 
      SqrtBox["\[Pi]"]], "+", 
     RowBox[{"4", " ", 
      SqrtBox[
       FractionBox["2", 
        RowBox[{"5", " ", "\[Pi]"}]]], " ", "R", " ", "\[Rho]", " ", 
      RowBox[{"Csc", "[", "\[Theta]", "]"}], " ", 
      RowBox[{"Sin", "[", 
       FractionBox["\[Pi]", "n"], "]"}], " ", 
      SqrtBox[
       FractionBox[
        RowBox[{"P", " ", 
         SuperscriptBox["R", "2"], " ", 
         SuperscriptBox[
          RowBox[{"Csc", "[", "\[Theta]", "]"}], "2"], " ", 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           FractionBox["\[Pi]", "n"], "]"}], "2"]}], 
        RowBox[{"EE", " ", "n"}]]]}]}], ")"}], " ", 
   RowBox[{"Tan", "[", "\[Theta]", "]"}]}], 
  RowBox[{"2", " ", "R"}]]], "Output",
 CellChangeTimes->{
  3.7907876649166327`*^9, 3.7907880969607964`*^9, {3.790788359356959*^9, 
   3.7907883862351966`*^9}, 3.7907888946670218`*^9, 3.790789775675726*^9},
 CellLabel->
  "Out[224]=",ExpressionUUID->"14cbbc3a-ed9d-4b39-8498-eded0fc9829c"]
}, Open  ]],

Cell["\<\
Now that is super nasty so lets see if we can make it a bit nicer...\
\>", "Text",
 CellChangeTimes->{{3.7907784094312534`*^9, 
  3.7907784311202674`*^9}},ExpressionUUID->"0eb69352-0c7e-4080-90d0-\
d1cc98bc3400"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Together", "[", "w", "]"}]], "Input",
 CellChangeTimes->{{3.790777745404689*^9, 3.7907777548248687`*^9}, {
  3.790777791918329*^9, 3.790777793672447*^9}, {3.7907782777099075`*^9, 
  3.7907782796885567`*^9}},
 CellLabel->
  "In[225]:=",ExpressionUUID->"69f3da5d-da07-454d-9b6c-c2dc19485e5b"],

Cell[BoxData[
 FractionBox[
  RowBox[{"2", " ", "\[Rho]", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"10", " ", 
      RowBox[{"Cot", "[", "\[Theta]", "]"}], " ", 
      SqrtBox[
       FractionBox[
        RowBox[{"P", " ", 
         SuperscriptBox["R", "2"], " ", 
         SuperscriptBox[
          RowBox[{"Cot", "[", "\[Theta]", "]"}], "2"], " ", 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           FractionBox["\[Pi]", "n"], "]"}], "2"]}], 
        RowBox[{"EE", " ", "n"}]]]}], "+", 
     RowBox[{
      SqrtBox["10"], " ", 
      RowBox[{"Csc", "[", "\[Theta]", "]"}], " ", 
      SqrtBox[
       FractionBox[
        RowBox[{"P", " ", 
         SuperscriptBox["R", "2"], " ", 
         SuperscriptBox[
          RowBox[{"Csc", "[", "\[Theta]", "]"}], "2"], " ", 
         SuperscriptBox[
          RowBox[{"Sin", "[", 
           FractionBox["\[Pi]", "n"], "]"}], "2"]}], 
        RowBox[{"EE", " ", "n"}]]]}]}], ")"}], " ", 
   RowBox[{"Tan", "[", "\[Theta]", "]"}]}], 
  RowBox[{"5", " ", 
   SqrtBox["\[Pi]"]}]]], "Output",
 CellChangeTimes->{
  3.7907881173658934`*^9, {3.790788359451704*^9, 3.7907883863099957`*^9}, 
   3.7907888995481787`*^9, 3.790789775763486*^9},
 CellLabel->
  "Out[225]=",ExpressionUUID->"7d90c8b1-4847-479f-b022-b0acc1cf364a"]
}, Open  ]],

Cell["\<\
And after some more massaging we can express the thing above as\
\>", "Text",
 CellChangeTimes->{{3.790778436305604*^9, 
  3.790778460748444*^9}},ExpressionUUID->"3e00672f-861b-41e5-824e-\
8b6454af9caf"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"K", "=", 
   FractionBox["2", 
    RowBox[{"5", 
     SqrtBox["2"]}]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Mu]R", "=", "R"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Mu]P", "=", 
   SqrtBox["P"]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Mu]m", "=", 
   FractionBox["\[Rho]", 
    SqrtBox["EE"]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Mu]n", "[", "n_", "]"}], "=", 
   FractionBox[
    RowBox[{"Sin", "[", 
     FractionBox["\[Pi]", "n"], "]"}], 
    SqrtBox["n"]]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Mu]theta", "[", "\[Theta]_", "]"}], "=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"10", 
      RowBox[{"Cot", "[", "\[Theta]", "]"}]}], "+", 
     FractionBox[
      SqrtBox["10"], 
      RowBox[{
       RowBox[{"Sin", "[", "\[Theta]", "]"}], 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}]]}], ")"}]}], 
  ";"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.790786847745159*^9, 3.7907870768859406`*^9}, {
  3.7907895118469324`*^9, 3.7907895706928473`*^9}, {3.7908807944412003`*^9, 
  3.7908807967699747`*^9}, {3.7908814330979657`*^9, 3.7908814365381145`*^9}},
 CellLabel->"In[36]:=",ExpressionUUID->"289622fa-e151-45ce-8d4b-0bf1798430d0"],

Cell[BoxData[
 RowBox[{
  RowBox[{"w", "\[Equal]", 
   RowBox[{
   "K", "*", "\[Mu]R", "*", "\[Mu]P", "*", "\[Mu]m", "*", "\[Mu]n", "*", 
    "\[Mu]theta"}]}], ";"}]], "Input",
